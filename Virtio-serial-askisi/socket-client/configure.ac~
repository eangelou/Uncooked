#                                               -*- Autoconf -*-
# Process this file with autoconf to produce a configure script.

AC_PREREQ([2.68])
AC_INIT([socket-client], [0.1], [test])
AC_CONFIG_SRCDIR([test.c])
AC# Checks for programs.
AC_PROG_CC

# Checks for libraries.

# Checks for programs.
AC_PROG_CC

# Checks for libraries.

# Checks for header files.
AC_CHECK_HEADERS([fcntl.h netdb.h netinet/in.h stdlib.h string.h sys/ioctl.h sys/socket.h termios.h unistd.h utmp.h pty.h])

# Checks for typedefs, structures, and compiler characteristics.

# Checks for library functions.
AC_FUNC_ERROR_AT_LINE
AC_CHECK_FUNCS([bzero gethostbyname memset socket openpty])

AC_CONFIG_FILES([Makefile])

AM_INIT_AUTOMAKE

AC_OUTPUT
